
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Dashboard - SB Admin</title>
    <link href="~/Assets/admin/css/styles.css" rel="stylesheet" />
    <link href="~/Content/Common/css/common.css" rel="stylesheet" />
    <link href="~/Assets/admin/css/GeneralProperties.css" rel="stylesheet" />

    @RenderSection("cssHeader", required: false)
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-grid.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-reboot.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Content/alertifyjs/alertify.min.css" rel="stylesheet" />
    <link href="~/Content/alertifyjs/themes/default.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-select.min.css" rel="stylesheet" />

    <script src="~/fonts/fontawesome/js/all.min.js"></script>
    <script src="~/Scripts/jquery-3.5.1.min.js"></script>
    <script src="~/Scripts/umd/popper.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/bootstrap-select.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/moment.min.js"></script>
    @RenderSection("jsHeader", required: false)

    <link href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" crossorigin="anonymous" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/js/all.min.js" crossorigin="anonymous"></script>
</head>
<body class="sb-nav-fixed">
    @RenderPage("_Header.cshtml")
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            @RenderPage("_SidebarLeft.cshtml")
        </div>
        <div id="layoutSidenav_content">
            @RenderSection("breadcrumb", required: false)
            <main style=" padding: 0 10px;">
                @RenderBody()
            </main>
            <footer class="py-4 bg-light mt-auto">
                @RenderPage("_Footer.cshtml")
            </footer>
        </div>
    </div>

    <script src="~/Assets/admin/js/scripts.js"></script>
    @RenderSection("jsFooter", required: false)

    <div class="backdrop__body-backdrop___1rvky">
        <div class="css-1272naz"></div>
        <div class="icon-loading">
            <img src="~/Data/images/IconLoading/sample.gif" alt="Alternate Text" style="width: 100px; height: 100px; border-radius: 50%;"/>
        </div>
    </div>
</body>
</html>
